Menu.removeClass('bottom')\n        }\n    }\n    \n    $(window).on('scroll', leftSticky)\n    $('.link').on('click', function() {\n        setTimeout(leftSticky, 550)\n    })\n    \n})\n\n<\/script>","background_pseudo_before_image":{"url":"","id":""},"background_pseudo_before_video_fallback":{"url":"","id":""},"background_pseudo_before_slideshow_gallery":[],"background_pseudo_after_image":{"url":"","id":""},"background_pseudo_after_video_fallback":{"url":"","id":""},"background_pseudo_after_slideshow_gallery":[]},"elements":[],"isInner":false,"widgetType":"html","elType":"widget"}],"isInner":true,"elType":"column"}],"isInner":true,"elType":"section"}],"isInner":false,"elType":"column"},{"id":"38b01d6","settings":{"_column_size":33,"_inline_size":3.666,"space_between_widgets":0,"background_background":"classic","padding":{"unit":"px","top":"0","right":"0","bottom":"0","left":"0","isLinked":true},"background_pseudo_before_image":{"url":"","id":""},"background_pseudo_before_video_fallback":{"url":"","id":""},"background_pseudo_before_slideshow_gallery":[],"background_pseudo_after_image":{"url":"","id":""},"background_pseudo_after_video_fallback":{"url":"","id":""},"background_pseudo_after_slideshow_gallery":[]},"elements":[],"isInner":false,"elType":"column"},{"id":"38347567","settings":{"_column_size":33,"_inline_size":77.666,"_inline_size_tablet":74,"background_background":"classic","background_color":"#FFFFFF","border_radius":{"unit":"px","top":"14","right":"14","bottom":"14","left":"14","isLinked":true},"padding":{"unit":"px","top":"30","right":"30","bottom":"30","left":"30","isLinked":true},"css_classes":"content","background_pseudo_before_image":{"url":"","id":""},"background_pseudo_before_video_fallback":{"url":"","id":""},"background_pseudo_before_slideshow_gallery":[],"background_pseudo_after_image":{"url":"","id":""},"background_pseudo_after_video_fallback":{"url":"","id":""},"background_pseudo_after_slideshow_gallery":[],"space_between_widgets":0},"elemen